{
  "rewrites": [
  { "source": "/dashboard", "destination": "/dashboard" },
  { "source": "/(.*)", "destination": "/" }
],
  "headers":[
    {
      "source": "/dashboard",
      "headers": [  
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }]
}