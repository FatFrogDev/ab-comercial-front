{
  "rewrites": [
  { "source": "/dashboard", "destination": "/dashboard" },
  { "source": "/(.*)", "destination": "/" }
],
"routes":[
  {
    "src": "/(.*)",
    "methods":[ "GET", "POST", "PUT", "DELETE", "PATCH"]
  }],
  "headers":[
    {
      "source": "/dashboard",
      "headers": [  
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }]
}